# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

/node_modules
/yarn-error.log

.byebug_history

# Ignore coffee
app/assets/javascripts/*.coffee

# Ignore scss
app/assets/stylesheets/application.scss

; unknown
"\342\200\235/"

# アップロードされたテスト画像を無視する
/public/uploads
/heroku

# 機密情報
/config/secrets.yml

# 昔のファイルのバックアップ
/shelter
# Ignore master key for decrypting credentials and more.
/config/master.key

# 環境変数
.env